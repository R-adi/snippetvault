{
  "name": "snippetvault",
  "displayName": "SnippetVault",
  "description": "Crafting and saving Code Snippets Made Simple",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.96.0"
  },
"repository": {
  "type": "git",
  "url": "https://github.com/R-adi/snippetvault"
},



  "publisher": "snippet-vault",
  "icon": "./assets/snippetvault_logo.png",


  "categories": [
    "Snippets",
    ""
  ],
  "keywords": [
    "snippets",
    "code snippets",
    "productivity",
    "reusable code",
    "snippet storage"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      
      {
        "command": "extension.addSnippet",
        "title": "add snippet"
      }
    ],
    "keybindings": [
      {
        "command": "extension.addSnippet",
        "key": "ctrl+shift+n",
        "mac": "cmd+shift+n",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.16.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
